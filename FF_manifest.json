{
   "manifest_version": 3,
   "name": "Replika Export",
   "version": "1.0.0",
   "description": "Export your Replika chat messages, diary, and memory entries.",
   "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
   },
   "action": {
      "default_icon": {
         "16": "icons/16.png",
         "32": "icons/32.png",
         "48": "icons/48.png",
         "128": "icons/128.png"
      }
   },
   "background": {
      "scripts": [
         "background-script.js"
      ]
   },
   "permissions": [
      "contextMenus",
      "scripting",
      "storage",
      "activeTab",
      "webNavigation",
      "tabs"
   ],
   "host_permissions": [
      "https://my.replika.com/*",
      "https://my.replika.ai/*"
   ],
   "author": "replika@creepyna.me",
   "browser_specific_settings": {
      "gecko": {
         "id": "replika@creepyna.me"
      }
   }
}